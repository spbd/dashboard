{
    "browser": true,
    "jquery": true,
    "node": true,

    "bitwise": true,
    "freeze": true,
    "camelcase": true,
    "eqeqeq": true,
    "es3": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
